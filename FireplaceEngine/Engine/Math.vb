Public Class Math

    Public Function add2(one As Integer, two As Integer) As Integer
        Return one + two
    End Function

    Public Function sub2(one As Integer, two As Integer) As Integer
        Return one - two
    End Function

    Public Function divide2(one As Integer, two As Integer) As Integer
        Return one / two
    End Function

    Public Function times2(one As Integer, two As Integer) As Integer
        Return one * two
    End Function

End Class